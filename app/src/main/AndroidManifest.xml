<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.schooner.edtechadvisor">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Tutorial"
            android:label="ToolBar Test"
            android:parentActivityName=".MainActivity" />
        />
        <activity
            android:name=".SuggestActivity"
            android:label="Suggest"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Category"
            android:label="Category"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Tutorials"
            android:label="@string/title_activity_tutorials"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AdminTools"
            android:label="Adminsitrative Tools"
            android:parentActivityName=".Category"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".ContentTools"
            android:label="Content-Based Tools"
            android:parentActivityName=".Category"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".ClassDojo"
            android:label="Class Dojo"
            android:parentActivityName=".ContentTools"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".SignUp"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".SampleTool"
            android:parentActivityName=".Category"
            android:theme="@style/AppTheme" />
        <activity android:name=".LoginActivity"
            android:parentActivityName=".MainActivity"
            ></activity>
    </application>

</manifest>